
<div align="center">	
<fieldset>
	<form name="altaimplantform" id="altaimplantform" class="register f2">
	
	<h5>
	<legend><a><img src="../../static/img/v2.0/img/agregarimplant.jpg" height="30" width="30" id="imgImplants2" name="imgImplants2"> INGRESO LOS DATOS</a></legend>	
		</h5>
		<table>
			<tr>
		    <td>			   			   
	   			<label>NOMBRE:</label>
	   		</td>
	   		<td>
	   			<input maxlength="100" type="text" id="nombreImp" name="nombreImp" data-validation="required" onkeyup="javascript:this.value=this.value.toUpperCase();"/>
	   		</td>
	   		</tr>
	   		<tr>	   		
	   		<td>
				<label>APELLIDO PATERNO:</label>
			</td>
			<td>
			  <input maxlength="50" type="text" id="appImp" name="appImp" data-validation="required" onkeyup="javascript:this.value=this.value.toUpperCase();"/>	    	
		    </td>
	   		</tr>  
			<tr>
			<td>
				<label>APELLIDO MATERNO:</label>
			</td>
			<td>
			  <input maxlength="50" type="text" id="apmImp" name="apmImp" data-validation="required" onkeyup="javascript:this.value=this.value.toUpperCase();"/>	    	
		    </td>
		    </tr>
		    <tr>
		    <td>		   		
	   			<label>NEXTEL:</label>
	   		</td>
	   		<td>
	   		 <input maxlength="10" type="text" id="nextelImp" name="nextelImp" data-validation="required" onkeyup="javascript:this.value=this.value.toUpperCase();"/>
	   		</td>
	   		 </tr>
	   		   <tr>
		    <td>		   		
	   			<label>ID:</label>
	   		</td>
	   		<td>
	   		 <input maxlength="25" type="text" id="idNextImp" name="idNextImp" data-validation="required" onkeyup="javascript:this.value=this.value.toUpperCase();"/>
	   		</td>
	   		 </tr>
	   		   <tr>
		    <td>		   		
	   			<label>CELULAR:</label>
	   		</td>
	   		<td>
	   		 <input maxlength="10" type="text" id="celularImp" name="celularImp" data-validation="required" onkeyup="javascript:this.value=this.value.toUpperCase();"/>
	   		</td>
	   		 </tr>
	   		   <tr>
		    <td>		   		
	   			<label>TELÉFONO OFICINA:</label>
	   		</td>
	   		<td>
	   		 <input maxlength="10" type="text" id="telOfImp" name="telOfImp" data-validation="required" onkeyup="javascript:this.value=this.value.toUpperCase();"/>
	   		</td>
	   		 </tr>
	   		   <tr>
		    <td>		   		
	   			<label>PUESTO:</label>
	   		</td>
	   		<td>
	   		 <input maxlength="100" type="text" id="puestoImp" name="puestoImp" data-validation="required" onkeyup="javascript:this.value=this.value.toUpperCase();"/>
	   		</td>
	   		 </tr>
	   		   <tr>
		    <td>		   		
	   			<label>CORREO ELECTRÓNICO INSTITUCIONAL:</label>
	   		</td>
	   		<td>
	   		  <input  maxlength="100" type="text" id="emailInstImp" name="emailInstImp" data-validation="required email" onkeyup="javascript:this.value=this.value.toUpperCase();"/>
	   		</td>
	   		 </tr>
	   		   <tr>
		    <td>		   		
	   			<label>CORREO ELECTRÓNICO PERSONAL:</label>
	   		</td>
	   		<td>
	   		 <input maxlength="100" type="text" id="emailPersImp" name="emailPersImp" data-validation="required email" onkeyup="javascript:this.value=this.value.toUpperCase();"/>
	   		</td>
	   		 </tr>
	   		 <tr>
		    <td>		   		
	   			<label>¿SUPER MÉDICO?:</label>
	   		</td>
	   		<td>
	   		 <input type="checkbox" id="supermedicoImp" name="supermedicoImp"  onkeyup="javascript:this.value=this.value.toUpperCase();" />
	   		</td>
	   		 </tr>
	   	</table>	
		<p>
		   <div align="center"><button type="submit" id="buttonGuardarEvento" class="myButton">Guardar</button></div>							
		</p>
	</form>	
</fieldset>
</div>	